        strSql = strSql & "[Year]    		TEXT(4),"
        strSql = strSql & "MonthDay			TEXT(4),"
        strSql = strSql & "Kumi		    	TEXT(10),"
        strSql = strSql & "PayJyushosiki    TEXT(9),"
        strSql = strSql & "TekichuHyo		TEXT(10),"
        strSql = strSql & "CONSTRAINT JYUSYOSIKI PRIMARY KEY ([Year],MonthDay,Kumi));"

        objCN.Execute(strSql)

        '競走馬除外情報
        strSql = "CREATE TABLE JOGAIBA ("
        strSql = strSql & "RecordSpec       TEXT(2),"
        strSql = strSql & "DataKubun        TEXT(1),"
        strSql = strSql & "MakeDate         TEXT(8),"
        strSql = strSql & "[Year]           TEXT(4),"
        strSql = strSql & "MonthDay         TEXT(4),"
        strSql = strSql & "JyoCD            TEXT(2),"
        strSql = strSql & "Kaiji            TEXT(2),"
