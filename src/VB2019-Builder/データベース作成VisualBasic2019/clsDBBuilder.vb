    End Function


    ' @(f)
    '
    ' 機能      :　DB削除
    '
    ' 引き数    :  strFilePath ファイルパス
    '
    ' 返り値    :
    '
    ' 機能説明  :  指定したパスにDBを作成する。
    '
    Public Function KillDB(ByVal strFilePath As String) As Boolean
        On Error GoTo ErrorHandler

        'ファイルの存在チェック

        If Len(Dir(strFilePath)) = 0 Then
            MsgBox("指定されたファイルは存在しません。")
            Exit Function
        End If

        Kill(strFilePath)
        KillDB = True

ExitHandler:
        Exit Function
ErrorHandler:
        KillDB = False
        System.Diagnostics.Debug.WriteLine(Err.Description)
        Resume ExitHandler
    End Function


    ' @(f)
    '
    ' 機能      :　DB最適化
    '
    ' 引き数    :  strFilePath ファイルパス
    '
    ' 返り値    : True - 成功, False - 失敗
    '
    ' 機能説明  : DBを最適化する。
    '
    Public Function CompactDB(ByVal strFilePath As String) As Boolean
        On Error GoTo ErrorHandler
        Dim myjet As Object
        Dim strCon As String '接続文字列
        Dim strFilePath_tmp As String

        '接続文字列
        strCon = "Provider=Microsoft.ACE.OLEDB.16.0;Data Source="

        'データベースタイプを設定（Type=4:Access97　Type=5:Access2000)
        strFilePath = strFilePath

        'Tmpデータベース名指定
        Dim i As Short
        Do
            'Tmpデータベース名指定
            strFilePath_tmp = strFilePath & i
            i = i + 1
        Loop Until Len(Dir(strFilePath_tmp)) = 0 ''Tmpデータベースの存在チェック

        'データベースの最適化
        myjet = CreateObject("DAO.DBEngine.120")
        myjet.CompactDatabase(strFilePath, strFilePath_tmp)

        '解放

        myjet = Nothing

        'ファイルコピー
        Call FileCopy(strFilePath_tmp, strFilePath)

        'Tmpファイルを削除
        Kill(strFilePath_tmp)

        CompactDB = True

ExitHandler:
        Exit Function
ErrorHandler:
        CompactDB = False
        System.Diagnostics.Debug.WriteLine(Err.Description)
        Resume ExitHandler
    End Function
End Class